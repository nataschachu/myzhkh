<!DOCTYPE html>
<html>
<head>
    <title>Тарифы</title>
    <% include ../partials/header.ejs %>
    <link rel="stylesheet" type="text/css" href="/stylesheets/tarif.css" />
</head>
<body>
<div class="container">
    <h2>Тарифы ЖКХ</h2>
    <table class="tarif-table">
        <% results.forEach(function(r) { %>
            <tr>
                <td><%= r.tarif_name %></td>
                <td><%= r.tarif_value %></td>
                <td data-tarif="<%= r.tarif_name %>"><span class="change">Изменить</span></td>
            </tr>
        <% }); %>            
    </table>
</div>
<script src="http://yastatic.net/jquery/1.8.3/jquery.min.js"></script>
<script src="/js/tarif.js"></script>
</body>
</html>
